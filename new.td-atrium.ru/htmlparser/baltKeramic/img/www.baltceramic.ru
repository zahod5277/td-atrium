				$('#'+ next).find('button.show-modal').trigger('click');
		});
		
		$(document).on('click', '.cat-arrow-l', function() {
				$.arcticmodal('close');
				key = getKey(list, $(this).data('id'));
				if(key == 0) { prev = list[list.length - 1] } else { prev = list[key - 1] }
				$('#'+ prev).find('button.show-modal').trigger('click');
		});


